{
  "name": "TradeX",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "check": "gts check",
    "fix": "gts fix",
    "clean": "rimraf build",
    "compile": "yarn clean && yarn run tsc",
    "prepare": "yarn compile",
    "pretest": "yarn compile",
    "posttest": "yarn check",
    "react-devtools": "react-devtools",
    "ios_list": "xcrun simctl list devices",
    "ios_bundle": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "ios": "react-native run-ios",
    "ios8": "react-native run-ios --simulator=\"iPhone 8\"",
    "ios8p": "react-native run-ios --simulator=\"iPhone 8 Plus\"",
    "ios11": "react-native run-ios --simulator=\"iPhone 11\"",
    "ios11p": "react-native run-ios --simulator=\"iPhone 11 Pro\"",
    "ios11pm": "react-native run-ios --simulator=\"iPhone 11 Pro Max\"",
    "ios12": "react-native run-ios --simulator=\"iPhone 12\"",
    "ios12m": "react-native run-ios --simulator=\"iPhone 12 mini\"",
    "ios12p": "react-native run-ios --simulator=\"iPhone 12 Pro\"",
    "ios12pm": "react-native run-ios --simulator=\"iPhone 12 Pro Max\"",
    "android_bundle": "mkdir -p android/app/src/main/assets && react-native bundle --platform android --dev false --minify --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android_clean": "rm -rf ./android/app/src/main/res/drawable-* && rm -f ./android/app/src/main/res/raw/*",
    "android_build_debug": "yarn compile && rm -f android/app/build/outputs/apk/debug/* && yarn android_bundle && yarn android_clean && cd android && ./gradlew app:assembleDebug && cd .. && mkdir -p output && cp -f android/app/build/outputs/apk/debug/*.apk ./output",
    "android_build_release": "yarn compile && yarn android_bundle && yarn android_clean && cd android && ./gradlew clean app:assembleRelease && cd .. && mkdir -p output && cp -f android/app/build/outputs/apk/release/*.apk ./output",
    "android_build_bundle": "yarn compile && yarn android_bundle && yarn android_clean && cd android && ./gradlew clean  bundleRelease && cd .. && mkdir -p output && cp -f android/app/build/outputs/bundle/release/*.aab ./output",
    "android": "yarn compile && yarn android_bundle && react-native run-android",
    "android_release": "yarn compile && yarn android_bundle && react-native run-android --variant release",
    "android_install_debug": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug && cd ..",
    "android_install_release": "cd ./android && ./gradlew app:assembleRelease && ./gradlew installRelease && cd ..",
    "android_codepush_kis": "appcenter codepush release-react -a KISV/KIS-MTS-Android --target-binary-version \"1.0.x\" -d Production",
    "ios_codepush_kis": "appcenter codepush release-react -a KISV/KIS-MTS-IOS --plist-file \"ios/TradeX/TradeX.plist\" -d Production",
    "codepush_kis": "yarn android_codepush_kis && ios_codepush_kis",
    "testfairy": "testfairy() { curl https://upload.testfairy.com/api/upload -F api_key='506940891eb347e6e0600b9d5cf274f304dfa6ce' -F file=@\"$@\";}; testfairy"
  },
